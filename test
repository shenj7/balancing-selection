initialize() {setSeed(0);initializeMutationRate0;initializeMutationType("m1", 0.5, "f", 0.0); initializeMutationType("m2", 0.5, "f", 0.1); initializeGenomicElementType("g1", m1, 1.0);initializeGenomicElementType("g2", c(m1, m2), c(999, 1);initializeGenomicElement(g1, 0, 32);initializeGenomicElement(g2, 33, 66);initializeGenomicElement(g1, 67, 99);initializeRecombinationRate0;}1 early() {sim.addSubpop(p1, 100);}10000 early() { sim.simulationFinished();p1.genomes.outputVCF(filePath="runs/test");}