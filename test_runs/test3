initialize() {setSeed(10);initializeMutationRate(0.5);initializeMutationType("m1", 0.5, "f", 0.0); initializeMutationType("m2", 0.5, "f", 0.1); initializeGenomicElementType("g1", m1, 1.0);initializeGenomicElementType("g2", c(m1, m2), c(999, 1);initializeGenomicElement(g1, 0, 32);initializeGenomicElement(g2, 33, 66);initializeGenomicElement(g1, 67, 99);initializeRecombinationRate(0.01);}1 early() {sim.addSubpop(p1, 1000);}10000 early() { sim.simulationFinished();p1.genomes.outputVCF(filePath="runs/test");}